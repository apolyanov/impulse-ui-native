    "@impulse-ui-native/icon": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-native": ">=0.74",
    "expo": ">=51 || >=52",
    "react-native-worklets": ">=0.5.1",
    "expo-modules-core": ">=1.0.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-gesture-handler": ">=2",
    "react-native-reanimated": ">=3"
  },
  "publishConfig": {
    "access": "public"
  }
